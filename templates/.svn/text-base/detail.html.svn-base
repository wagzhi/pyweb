  <header id="header">
  	<div style="width:980px; height:100%;  margin:0 auto;">
  	<span  style="
    	width:200px; 
        height:100%; 
        display:inline-block;
        float:left;  
        font-size:14px">
    	<a href="http://www.19lou.com">
        	<img src="{{ STATIC_URL }}img/title.png" height="30px"></img>
   		</a>
   </span>
   <nav class="ui-tabs-nav" style="height:100% !important">
   	<ul>
    	<li><a href="">入城口</a></li><li><a href="">我的空间</a></li><li><a href="">好店</a></li><li><a href="">客户端</a></li>
    </ul>
   </nav>
   <span style="float:right;">   
  <a href="http://www.19lou.com" id="header-user-url">猪猪侠</a>
  <img src="{{ STATIC_URL }}img/avataor.jpg" height="35" style="padding-left:10px; padding-bottom:5px"></img>       
   
    </span>
    </div>
  </header>
  <div role="main" id="main">
  	{% for doc in docs %}{% if doc.num_photo > 1 %}<div class="article2">{% else %}<div class="article">{% endif %}
  		<div class='article_title' style='font:22px "Microsoft Yahei"; width=100%'>
  			<a href='{{doc.url}}' target="_blank">{{doc.name}}</a></div>
  		<div>{{doc.digest}}</div>
  		{% if doc.num_photo > 0%}
			
				 {% if doc.photo1 %}
				 	<img src='{{doc.photo1}}'/>
				 {% endif %}
				 {% if doc.photo2 %}
				 	<img src='{{doc.photo2}}'/>
				 {% endif %}
				 {% if doc.photo3 %}
				 	<img src='{{doc.photo3}}'/>
				 {% endif %}
				
		{% endif %}  		
  	</div>{% endfor %}
   <footer>

  </footer>


  <!-- JavaScript at the bottom for fast page loading -->

  <!-- Grab Google CDN's jQuery, with a protocol relative URL; fall back to local if offline -->
  <script src="{{ STATIC_URL }}js/jquery-1.8.3.js"></script>
